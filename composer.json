{
    "name": "shrimpliu/elastic-scout",
    "description": "Elastic Driver for Laravel Scout",
    "keywords": ["laravel", "scout", "elasticsearch", "elastic"],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": ">=5.4.0",
        "laravel/scout": "^3.0",
        "elasticsearch/elasticsearch": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "ShrimpLiu\\ElasticScout\\": "src/"
        }
    },
    "suggest": {

    },
    "extra": {
        "laravel": {
            "providers": [
                "ShrimpLiu\\ElasticScout\\ElasticScoutServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
